{
  "name": "cash-machine-js",
  "version": "1.0.0",
  "description": "Cash machine library",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinPugliese/cash-machine-js"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Kevin Pugliese <kevin.lima.contato@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinPugliese/cash-machine-js/issues"
  },
  "homepage": "https://github.com/kevinPugliese/cash-machine-js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "node-fetch": "^1.7.3",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0",
    "sinon-stub-promise": "^4.0.0"
  }
}
